[webservers]
webserver ansible_host=192.168.1.10

[databases]
database ansible_host=192.168.1.20

[production]
cloud ansible_host=www.ansidock.com

[production:vars]
ansible_ssh_user=root

# Group 'multi' with all servers
[multi:children]
webservers
databases

# Variables that will be applied to all servers
[multi:vars]
ansible_ssh_user=vagrant

